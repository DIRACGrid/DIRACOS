{
   "rpmBuild" :{
      "mockConfig": "mockConfig",
      "mockRoot": "mockRoot",
      "patchDir" : "patchDir",
      "packageGroups": [
         {
            "mockConfig": "grp1_mockConfig",
            "mockRoot": "grp1_mockRoot",
            "patchDir" : "grp1_patchDir",
            "name": "grp1",
            "packages": [
               {
                  "name" : "grp1_pkg1_name",
